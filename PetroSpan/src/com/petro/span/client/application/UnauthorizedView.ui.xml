<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
             <ui:with type="com.petro.span.client.resources.Resources"
		field="res"></ui:with>
		
		
   <g:LayoutPanel>
   
      <g:layer left ="10%" top = "15%" width ="40%"  height = "80%">
      
       <g:Image resource = "{res.unauthorized}" ></g:Image>
      </g:layer>
      
      <g:layer left ="55%" top = "30%" height = "70%" width ="45%">
         <g:HTMLPanel>
        You are not authorized to view this page.
        <g:Hyperlink ui:field="linkToLogin">Please login first</g:Hyperlink>
    </g:HTMLPanel>
      </g:layer>
      
   </g:LayoutPanel>
</ui:UiBinder>